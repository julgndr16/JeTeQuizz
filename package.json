{
  "name": "je-te-quizz",
  "packageManager": "yarn@4.2.2",
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@fastify/multipart": "^8.2.0",
    "@fastify/static": "^7.0.3",
    "@fastify/swagger": "^8.14.0",
    "@fastify/swagger-ui": "^3.0.0",
    "@fastify/type-provider-typebox": "^4.0.0",
    "@fontsource/roboto": "^5.0.13",
    "@mui/icons-material": "^5.15.18",
    "@mui/material": "^5.15.18",
    "@prisma/client": "^5.14.0",
    "@react-oauth/google": "^0.12.1",
    "@sinclair/typebox": "^0.32.22",
    "fastify": "^4.26.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.23.1"
  },
  "devDependencies": {
    "@types/node": "^20.12.12",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "eslint-plugin-unused-imports": "^3.1.0",
    "prettier": "^3.2.5",
    "prisma": "^5.14.0",
    "rimraf": "^5.0.7",
    "tsx": "^4.10.5",
    "typescript": "^5.4.5",
    "vite": "^5.2.11"
  },
  "scripts": {
    "dev:ui": "vite --port 3000 --open",
    "dev:server": "tsx --watch src/server/index.ts",
    "setup_prisma": "prisma db pull && prisma generate && echo Prisma setup done! || echo Prisma setup failed!",
    "clean": "rimraf dist && echo Clean done! || echo Clean failed!",
    "build:ui": "echo Build UI... && vite build --emptyOutDir && echo Build UI done! || echo Build UI failed!",
    "build:server": "echo Build Server... && tsc src/server/index.ts --outDir dist/server && echo Build Server done! || echo Build Server failed!",
    "build": "yarn clean && yarn build:ui && yarn build:server",
    "preview": "vite preview --port 5000 --open",
    "start": "SERVER_PORT=5001 && SERVER_HOST=localhost && node dist/server/index.js",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write src/**/*.{ts,tsx}"
  }
}
